<h3>Propane</h3>
  
<form class="form-inline" role="form">
  <div class="input-group">
    <div class="form-group">
      <input class="form-control" type="text" placeholder="{{ propaneType.placeholder }}" ng-model="propaneInput">
    </div>
    <div class="form-group">
      <select class='form-control' ng-model="propaneType" ng-options="type.name for type in propaneOptions">
      </select>          
    </div>
  </div>
</form>
  




