<p><strong>Natural Gas</strong></p>

<form class="form-inline" role="form">
  <div class="input-group">
    <div class="form-group">
      <input class="form-control" type="text" placeholder="{{ naturalType.placeholder }}" ng-model="naturalInput">
    </div>
    <div class="form-group">
      <select class='form-control' ng-model="naturalType" ng-options="type.name for type in naturalOptions">
      </select>          
    </div>
  </div>
</form>

